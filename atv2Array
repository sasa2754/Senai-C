#include <stdio.h>

int main(){
    int array[5], maior = 0;

    for (int i=0;i<5;i++){
        printf("Digite o %io numero: ", i+1);
        scanf("%i", &array[i]);
        if (array[i]>maior){
            maior = array[i];
        }
    }
    printf("O maior numero e: %i", maior);
    return 0;
}
